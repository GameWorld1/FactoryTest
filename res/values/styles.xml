<resources>

    <style name="AppTheme" parent="android:Theme.Light.NoTitleBar.Fullscreen" />
	<style name="AppWindowFull" parent="android:Theme.Light">
	    <item name="android:windowFullscreen">true</item>
	</style>
    <style name="test_result_Icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">90dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style name="testIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">125dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style  name="progressDialog" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style  name="water_bg" parent="@style/AppTheme">
         <item name="android:windowBackground">@drawable/water_logo</item>
<!--           <item name="android:windowBackground">@android:color/transparent</item> -->
    </style>

</resources>